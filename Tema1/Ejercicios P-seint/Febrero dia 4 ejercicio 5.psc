Algoritmo Ejercicio_5
	Definir num Como Entero;
	
	leer num;
	si num < 0 Entonces;
		
		Escribir num, " negativo";
		
	SiNo;
		
		si num > 0 Entonces;
			
			Escribir num, " positivo";
			
		SiNo;
			
			si num = 0 Entonces;
			
				Escribir num, " nulo";
				

			FinSi
		FinSi
	FinSi
	
	
FinAlgoritmo
